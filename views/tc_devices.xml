<odoo>
    <record id='tc_devices_view_form' model='ir.ui.view'>
        <field name="name">tc_devices.form</field>
        <field name="model">tc_devices</field>
        <field name="arch" type="xml">
            <form string="GPS">
                <group col="4" string="GPS Configuration">                   
                    <field name="name" />
                    <field name="lastupdate"/>
                    <field name="uniqueid"/>
                    <field name="icc"/>
                                        
                    <field name="phone"/>
                    <field name="disabled"/>
                    <field name="telcel"/>
                    <field name="signal"/>                   
                </group>                
            </form>
        </field>
    </record>
    <record id='tc_devices_view_tree' model='ir.ui.view'>
        <field name="name">tc_devices.tree</field>
        <field name="model">tc_devices</field>
        <field name="arch" type="xml">
            <tree string="Models">
                <field name="name" />
                <field name="uniqueid"/>
                <field name="icc"/>
                <field name="lastupdate"/>                    
                <field name="phone"/>
                <field name="disabled"/>
                <field name="telcel"/>
                <field name="signal"/>
            </tree>
        </field>
    </record>
</odoo>
